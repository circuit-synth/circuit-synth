# Fast Generation API Keys
# OpenRouter API Key (required for fast generation)
OPENROUTER_API_KEY=your_openrouter_key_here

# Google Cloud Project Settings (optional, for direct Vertex AI)
GOOGLE_CLOUD_PROJECT=your_project_id
GOOGLE_CLOUD_LOCATION=us-central1
GOOGLE_GENAI_USE_VERTEXAI=FALSE

# Model Configuration
DEFAULT_FAST_MODEL=google/gemini-2.5-flash
FALLBACK_MODEL=google/gemini-2.0-flash-experimental

# Circuit Generation Settings
FAST_GENERATION_ENABLED=TRUE
CACHE_GENERATED_CIRCUITS=TRUE
VALIDATE_KICAD_COMPONENTS=TRUE